#include <stdio.h>

int main() {
int x, y, z, temp;
    printf("Uc sayi giriniz: ");
    scanf("%d %d %d", &x, &y, &z);

    if (x > y) { temp = x; x = y; y = temp; }
    if (x > z) { temp = x; x = z; z = temp; }
    if (y > z) { temp = y; y = z; z = temp; }

    printf("Kucukten Buyuge siralanis: %d %d %d\n", x, y, z);
}
